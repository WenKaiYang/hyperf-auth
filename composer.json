{
  "name": "ella123/hyperf-auth",
  "type": "library",
  "license": "MIT",
  "keywords": [
    "php",
    "hyperf",
    "hyperf-auth",
    "jwt-auth"
  ],
  "description": "",
  "autoload": {
    "psr-4": {
      "ELLa123\\HyperfAuth\\": "src/"
    },
    "files": [
      "src/helper.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "HyperfTest\\": "tests"
    }
  },
  "require": {
    "php": ">=8.0",
    "ella123/hyperf-jwt": "dev-master",
    "hyperf/cache": "2.2.*",
    "hyperf/command": "2.2.*",
    "hyperf/config": "2.2.*",
    "hyperf/database": "2.2.*",
    "hyperf/di": "2.2.*",
    "hyperf/event": "2.2.*",
    "hyperf/framework": "2.2.*",
    "hyperf/redis": "2.2.*",
    "hyperf/session": "2.2.*",
    "hyperf/testing": "2.2.*",
    "hyperf/utils": "2.2.*"
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^3.0",
    "mockery/mockery": "^1.0",
    "phpstan/phpstan": "^1.0",
    "phpunit/phpunit": ">=7.0",
    "swoole/ide-helper": "^4.5"
  },
  "suggest": {
    "swow/swow": "Required to create swow components.",
    "ext-redis": "*"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "optimize-autoloader": true,
    "sort-packages": true
  },
  "scripts": {
    "test": "phpunit -c phpunit.xml --colors=always",
    "analyse": "phpstan analyse --memory-limit 1024M -l 0 ./src",
    "cs-fix": "php-cs-fixer fix $1"
  },
  "extra": {
    "hyperf": {
      "config": "ELLa123\\HyperfAuth\\ConfigProvider"
    }
  }
}
